{
    "tags": {
        "optimizer-page": {
            "renderer": "./taglib/page-tag",
            "transformer": "./taglib/page-tag-transformer",
            "attributes": {
                "name": "string",
                "cache-key": "string",
                "package-path": "string",
                "package-paths": "expression",
                "base-path": "string",
                "optimizer": "expression",
                "data": "expression",
                "dependencies": "expression",
                "flags": "expression",
                "enabled-extensions": {
                    "target-property": "flags",
                    "type": "expression"
                },
                "extensions": {
                    "target-property": "flags",
                    "type": "expression"
                },
                "wait-for": "expression",
                "timeout": "integer"
            },
            "var": "_dependenciesParent",
            "import-var": {
                "dirname": "__dirname"
            }
        },
        "optimizer-dependencies": {
            "renderer": "./taglib/dependencies-tag"
        },
        "optimizer-dependency": {
            "renderer": "./taglib/dependency-tag",
            "import-var": {
                "dependenciesParent": "_dependenciesParent"
            },
            "attributes": {
                "*": "string"
            }
        },
        "optimizer-slot": {
            "renderer": "./taglib/slot-tag",
            "attributes": {
                "name": "string"
            }
        },
        "optimizer-head": {
            "renderer": "./taglib/head-tag"
        },
        "optimizer-body": {
            "renderer": "./taglib/body-tag"
        },
        "optimizer-img": {
            "renderer": "./taglib/img-tag",
            "attributes": {
                "src": "path",
                "base-path": "string",
                "optimizer": "expression",
                "*": "string"
            },
            "import-var": {
                "dirname": "__dirname"
            }
        }
    }
}
